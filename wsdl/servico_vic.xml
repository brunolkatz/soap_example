<definitions name = "ServicoTeste"
   targetNamespace = "http://localhost/wsdl/servico_vic.wsdl"
   xmlns = "http://schemas.xmlsoap.org/wsdl/"
   xmlns:soap = "http://schemas.xmlsoap.org/wsdl/soap/"
   xmlns:tns = "http://localhost/wsdl/servico_vic.wsdl"
   xmlns:xsd = "http://www.w3.org/2001/XMLSchema">
 
   <message name = "ServicoTesteRequest">
      <part name = "firstName" type = "xsd:string"/>
   </message>
	
   <message name = "ServicoTesteResponse">
      <part name = "greeting" type = "xsd:string"/>
   </message>

   <portType name = "servico_portType">
      <operation name = "funcionaServico">
         <input message = "tns:ServicoTesteRequest"/>
         <output message = "tns:ServicoTesteResponse"/>
      </operation>
   </portType>

   <binding name = "Hello_Binding" type = "tns:Hello_PortType">
      <soap:binding style = "rpc"
         transport = "http://schemas.xmlsoap.org/soap/http"/>
      <operation name = "funcionaServico">
         <soap:operation soapAction = "funcionaServico"/>
         <input>
            <soap:body
               encodingStyle = "http://schemas.xmlsoap.org/soap/encoding/"
               namespace = "urn:examples:helloservice"
               use = "encoded"/>
         </input>
		
         <output>
            <soap:body
               encodingStyle = "http://schemas.xmlsoap.org/soap/encoding/"
               namespace = "urn:examples:helloservice"
               use = "encoded"/>
         </output>
      </operation>
   </binding>

   <service name = "Hello_Service">
      <documentation>WSDL File for Simulador VIC Api</documentation>
      <port binding = "tns:Hello_Binding" name = "Hello_Port">
         <soap:address
            location = "http://localhost/soapexample/" />
      </port>
   </service>
</definitions>